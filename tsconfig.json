{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "allowJs": true,
    "target": "ES6",
    "module": "commonjs",
    "baseUrl": ".",
    "paths": {
      "ping/*": ["*"]
    }
  },
  "exclude": [
        "node_modules",
    ]
}
